    local I = 53
    local E = holoEntity(10)
    holoCreate(I)
    holoPos(I,E:toWorld(vec(-9,0,-8)))
    holoAng(I,E:toWorld(ang(90,0,0)))
    holoScale(I,vec(0.1,0.5,0.7))
    holoModel(I,"models/sprops/geometry/fhex_12.mdl")
    holoMaterial(I,Mat_Base)
    holoColor(I,Color_Base)
    holoParent(I,E)
    
    
}elseif(dupefinished()){
    reset()
}
